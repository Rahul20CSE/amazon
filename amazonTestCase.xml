<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">

<listeners>
    <listener class-name="com.crm.Listeners.WtihExtentReport"/>
  </listeners>
  <test name="InvalidSearchTest">
    <parameter name="search1" value="ld345tsxslfer"/>
    <classes>
      <class name="TestCase.RandomSerch"/>
    </classes>
  </test>
  <test name="ValidSearchTest">
    <parameter name="search2" value="laptop"/>
