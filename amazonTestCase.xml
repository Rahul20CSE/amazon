      <class name="TestCase.SearchProduct"/>
    </classes>
  </test>
  <test name="AddToCartTest">
    <classes>
      <class name="TestCase.addPtoCart"/>
    </classes>
  </test>
  <test name="UpdateCartTest">
    <classes>
      <class name="TestCase.updateCart"/>
    </classes>
  </test>
  <test name="EmptyCartTest">
    <classes>
      <class name="TestCase.emptyCart"/>
    </classes>
  </test>
</suite> <!-- Suite -->
